{
  "template": "journalbeat-*",
  "mappings": {
    "journal": {
      "properties": {
        "beat": {
          "properties": {
            "version": {
              "type": "keyword"
            },
            "name": {
              "type": "keyword"
            },
            "hostname": {
              "type": "keyword"
            }
          }
        },

        "type": {
          "type": "keyword"
        },
        "@realtime_timestamp": {
          "type": "long"
        },
        "@timestamp": {
          "format": "strict_date_optional_time||epoch_millis",
          "type": "date"
        },
        "message": {
          "type": "text"
        },

        "boot_id": {
          "type": "keyword"
        },
        "cap_effective": {
          "type": "keyword"
        },
        "catalog_entry": {
          "type": "keyword"
        },
        "cmdline": {
          "type": "keyword"
        },
        "code_file": {
          "type": "keyword"
        },
        "code_func": {
          "type": "keyword"
        },
        "code_line": {
          "type": "integer"
        },
        "comm": {
          "type": "keyword"
        },
        "exe": {
          "type": "keyword"
        },
        "gid": {
          "type": "integer"
        },
        "hostname": {
          "type": "keyword"
        },
        "leader": {
          "type": "keyword"
        },
        "machine_id": {
          "type": "keyword"
        },
        "message_id": {
          "type": "keyword"
        },
        "pid": {
          "type": "integer"
        },
        "priority": {
          "type": "integer"
        },
        "result": {
          "type": "keyword"
        },
        "session_id": {
          "type": "keyword"
        },
        "source_realtime_timestamp": {
          "type": "long"
        },
        "syslog_facility": {
          "type": "integer"
        },
        "syslog_identifier": {
          "type": "keyword"
        },
        "syslog_pid": {
          "type": "integer"
        },
        "systemd_cgroup": {
          "type": "keyword"
        },
        "systemd_invocation_id": {
          "type": "keyword"
        },
        "systemd_owner_uid": {
          "type": "integer"
        },
        "systemd_session": {
          "type": "keyword"
        },
        "systemd_slice": {
          "type": "keyword"
        },
        "systemd_unit": {
          "type": "keyword"
        },
        "systemd_user_slice": {
          "type": "keyword"
        },
        "transport": {
          "type": "keyword"
        },
        "uid": {
          "type": "integer"
        },
        "unit": {
          "type": "keyword"
        }
      }
    }
  }
}
